{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "Routers/userRouter.js"
        },
        {
            "src": "/movie/(.*)",
            "dest": "Routers/movieRouter.js"
        },
        {
            "src": "/series/(.*)",
            "dest": "Routers/seriesRouter.js"
        },
        {
            "src": "/posts/(.*)",
            "dest": "Routers/postRouter.js"
        },
        {
            "src": "/actors/(.*)",
            "dest": "Routers/actorsRouter.js"
        }
    ]
}